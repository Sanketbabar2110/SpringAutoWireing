<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
           <context:annotation-config/>
           
 <bean id="mobile" class="com.prowings.by_annotation1.Mobile">
 	<property name="name" value="oppo_reno"/>
 	<property name="prize" value="29000"/>
 </bean>
 
 <bean id="shopee" class="com.prowings.by_annotation1.Shopee">
 	<property name="name" value="Kirthi Mobile Shop"/>
 	<property name="street" value="Thakker bazzar"/>
 </bean>
 
</beans>
           
           
           